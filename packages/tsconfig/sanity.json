{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Sanity.io",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"strict": false,
		"noEmit": true,
		"module": "esnext",
		"jsx": "preserve"
	},
	"include": ["node_modules/@sanity/base/types/**/*.ts"],
	"exclude": ["node_modules"]
}
